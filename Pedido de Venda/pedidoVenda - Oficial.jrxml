	<pageHeader>
		<band height="61">
			<!-- * Parte Esquerda * -->
			<elementGroup/>
			<image isUsingCache="true">
				<reportElement x="0" y="0" width="126" height="40"/>
				<box>
					<rightPen lineWidth="0.0"/>
				</box>
				<imageExpression class="java.io.InputStream"><![CDATA[$P{logo}]]></imageExpression>
			</image>

			<textField>
				<reportElement x="130" y="0" width="263" height="10" forecolor="#333333"/>
				
				<textElement textAlignment="Left" verticalAlignment="Middle">
					<font fontName="SansSerif" size="7" isBold="true"/>
				</textElement>
				
				<textFieldExpression class="java.lang.String"><![CDATA[$F{empresa.nome}]]></textFieldExpression>
			</textField>

			<textField>
				<reportElement x="130" y="10" width="305" height="10" forecolor="#333333"/>
				
				<textElement verticalAlignment="Middle">
					<font fontName="SansSerif" size="7"/>
				</textElement>
				
				<textFieldExpression class="java.lang.String"><![CDATA["CNPJ: " + $F{empresa.cnpj} + " - IE: " + $F{empresa.inscricaoEstadual}]]></textFieldExpression>
			</textField>

			<textField isStretchWithOverflow="true">
				<reportElement x="130" y="20" width="332" height="10" forecolor="#333333"/>

				<textElement textAlignment="Left" verticalAlignment="Middle">
					<font fontName="SansSerif" size="7"/>
				</textElement>
				
				<textFieldExpression class="java.lang.String"><![CDATA[$F{empresa.endereco}+ ", " +
					$F{empresa.numero} + " - " +
					$F{empresa.bairro} + " \n" +
					$F{empresa.municipio} + " - " +
					$F{empresa.uf} + " - " +
					"CEP " + $F{empresa.cep}]]> 
				</textFieldExpression>
			</textField>

			<textField>
				<reportElement x="130" y="35" width="305" height="10" forecolor="#333333"/>
				
				<textElement verticalAlignment="Middle">
					<font fontName="SansSerif" size="7"/>
				</textElement>

				<textFieldExpression class="java.lang.String"><![CDATA["Telefone(s): " + $F{empresa.telefone}]]></textFieldExpression>
			</textField>

			<textField>
				<reportElement x="130" y="43" width="352" height="10" forecolor="#333333"/>

				<textElement verticalAlignment="Middle">
					<font fontName="SansSerif" size="7"/>
				</textElement>

				<textFieldExpression class="java.lang.String"><![CDATA["Site: " + $F{empresa.site}]]></textFieldExpression>
			</textField>

			<!-- ** Parte Direita ** -->
			<textField>
				<reportElement x="325" y="0" width="232" height="18" forecolor="#333333"/>
			
				<box>
					<pen lineWidth="1.0"/>
					<topPen lineWidth="0.0"/>
					<leftPen lineWidth="0.0"/>
					<bottomPen lineWidth="0.0"/>
					<rightPen lineWidth="0.0"/>
				</box>
				
				<textElement textAlignment="Right" verticalAlignment="Middle">
					<font fontName="SansSerif" size="10" isBold="true"/>
				</textElement>
				
				<textFieldExpression class="java.lang.String"><![CDATA["PEDIDO DE VENDA: "+$F{nome}]]></textFieldExpression>
			</textField>

			<textField pattern="dd/MM/yyyy">
				<reportElement x="444" y="18" width="113" height="12" isPrintWhenDetailOverflows="true" forecolor="#333333"/>

				<textElement textAlignment="Right" verticalAlignment="Middle">
					<font fontName="SansSerif" size="6" isBold="true"/>
				</textElement>

				<textFieldExpression class="java.util.Date"><![CDATA[$F{dataEmissao.time}]]></textFieldExpression>
			</textField>

			<textField>
				<reportElement x="355" y="37" width="200" height="10" forecolor="#333333">
